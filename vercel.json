{
    "version": 2,
    "builds": [
      { "src": "store-front/package.json", "use": "@now/next" },
      { "src": "dashboard/package.json", "use": "@now/next" }
    ],
    "routes": [
      { "src": "/dashboard/_next(.*)", "dest": "dashboard/_next$1" },
      { "src": "/dashboard(.*)", "dest": "dashboard/dashboard$1" },
      { "src": "(.*)", "dest": "store-front$1" }
    ]
  }